{
  "name": "@fujica/auto-i18n",
  "version": "0.1.11",
  "description": "A lightweight webpack plugin for automatic extraction and internationalization of Chinese text",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "require": "./lib/index.js",
      "types": "./lib/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testNamePattern='(?!.*integration)'",
    "test:integration": "jest integration.test.ts",
    "test:verbose": "jest --verbose",
    "format": "prettier --write \"src/**/*.{ts,js,vue}\" \"test-project/src/**/*.{ts,js,vue}\"",
    "release": "node scripts/release.js"
  },
  "keywords": [
    "webpack",
    "plugin",
    "i18n",
    "internationalization",
    "chinese",
    "translation"
  ],
  "author": "Fujica",
  "license": "MIT",
  "dependencies": {
    "@babel/generator": "^7.14.0",
    "@babel/parser": "^7.14.0",
    "@babel/traverse": "^7.14.0",
    "@babel/types": "^7.14.0",
    "minimatch": "^9.0.0",
    "axios": "^0.21.1",
    "chinese-conv": "^1.0.1",
    "crypto-js": "^4.0.0",
    "webpack-sources": "^1.4.3"
  },
  "devDependencies": {
    "@types/babel__traverse": "^7.18.0",
    "@types/crypto-js": "^4.0.0",
    "@types/jest": "^27.0.0",
    "@types/lodash": "^4.14.170",
    "@types/node": "^14.0.0",
    "jest": "^27.0.0",
    "ts-jest": "^27.0.0",
    "typescript": "^4.5.0",
    "webpack": "^4.0.0",
    "prettier": "^3.2.5"
  },
  "peerDependenciesMeta": {
    "webpack": {
      "optional": true
    }
  },
  "peerDependencies": {
    "webpack": "^4.0.0 || ^5.0.0"
  },
  "engines": {
    "node": ">=14.18.0"
  },
  "files": [
    "lib",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fujica/auto-i18n.git"
  },
  "bugs": {
    "url": "https://github.com/fujica/auto-i18n/issues"
  },
  "homepage": "https://github.com/fujica/auto-i18n#readme",
  "publishConfig": {
    "access": "public"
  }
}
