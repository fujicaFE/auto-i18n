{
  "name": "auto-i18n-test-project",
  "version": "1.0.0",
  "description": "Test project for auto-i18n webpack plugin",
  "scripts": {
    "serve": "set NODE_OPTIONS=--max-old-space-size=8192 && vue-cli-service serve",
    "serve:i18n": "set NODE_OPTIONS=--max-old-space-size=8192 && set DEV_I18N_MODE=i18n && vue-cli-service serve",
    "build": "npm run build:plugin && vue-cli-service build",
    "build:plugin": "cd .. && npm run build",
    "test:plugin": "npm run build && echo 'Plugin test completed'"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "vue": "^2.6.11",
    "vue-i18n": "^8.22.0",
    "vue-router": "^3.2.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "cross-env": "^10.1.0",
    "vue-template-compiler": "^2.6.11"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
